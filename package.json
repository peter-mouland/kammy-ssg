{
  "name": "kammy-ssg",
  "version": "0.1.0",
  "description": "UI components for Kammy",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "author": "peter.mouland@gmail.com",
  "license": "ISC",
  "homepage": "https://github.com/peter-mouland/kammy-ssg#readme",
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": "20.x",
    "npm": "10.2.0"
  },
  "bugs": {
    "url": "https://github.com/peter-mouland/kammy-ssg/issues"
  },
  "scripts": {
    "--- NETLIFY ---": "",
    "dev": "netlify dev",
    "deploy": "netlify deploy --prod --open --dir ./public",
    "--- GATSBY ---": "",
    "start": "TZ='Europe/London' gatsby develop",
    "start:static": "cd public; ws --spa index.html",
    "serve": "gatsby serve",
    "start:preview": "CI=true gatsby develop  --host 0.0.0.0",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "TZ='Europe/London' gatsby build --log-pages",
    "build:prod": "GATSBY_CPU_COUNT=2 NODE_ENV=production TZ='Europe/London' gatsby build --prefix-paths",
    "--- util ---": "",
    "clean": "yarn clean:yarn && lerna clean --yes && yarn clean:npm",
    "clean:dist": "rimraf public && gatsby clean",
    "clean:npm": "rimraf packages/**/node_modules && rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock && rimraf packages/**/yarn.lock",
    "fix:css-types": "tcm ./src",
    "verify": "yarn lint && yarn test",
    "nuke": "yarn clean && yarn && yarn verify",
    "lerna": "lerna",
    "--- testing ---": "",
    "check:css-types": "tcm --listDifferent .",
    "test": "jest --config=jest.config.js",
    "lint": "eslint --ext=js,jsx,ts,tsx",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "workspaces": [
    "packages/config/*",
    "packages/helpers/*",
    "packages/*"
  ],
  "dependencies": {
    "@tanstack/react-query": "5.77.2",
    "axios": "1.9.0",
    "chance": "1.1.13",
    "classnames": "2.5.1",
    "date-fns": "2.30.0",
    "dotenv": "16.5.0",
    "eslint-config-canonical": "41.4.2",
    "fromnow": "3.0.1",
    "gatsby": "5.14.3",
    "gatsby-adapter-netlify": "1.2.0",
    "gatsby-plugin-buildtime-timezone": "1.0.3",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-react-css-modules": "5.14.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-remove-trailing-slashes": "4.19.0",
    "gatsby-plugin-sass": "6.14.0",
    "gatsby-plugin-use-query-params": "1.0.1",
    "gatsby-source-filesystem": "5.14.0",
    "node-gyp": "9.4.1",
    "postcss-preset-env": "7.8.3",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-cookie": "4.1.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-select": "4.3.1",
    "recharts": "2.15.3",
    "rimraf": "5.0.10",
    "typed-css-modules": "0.9.1",
    "use-query-params": "2.2.1",
    "vercel": "^36.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "8.56.12",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/testing-library__jest-dom": "^5.14.9",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.14.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "lerna": "6.6.2",
    "local-web-server": "5.4.0",
    "typescript": "5.8.3"
  }
}
