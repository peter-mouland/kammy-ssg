{
  "name": "kammy-ssg",
  "version": "0.1.0",
  "description": "UI components + lambdas Kammy using lerna",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "author": "peter.mouland@gmail.com",
  "license": "ISC",
  "homepage": "https://github.com/peter-mouland/kammy-ssg#readme",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.9.0"
  },
  "bugs": {
    "url": "https://github.com/peter-mouland/kammy-ssg/issues"
  },
  "scripts": {
    "--- NETLIFY ---": "",
    "dev": "netlify dev",
    "deploy": "yarn build:prod && netlify deploy --prod --open --dir ./docs",
    "--- GATSBY ---": "",
    "start": "yarn clean:dist && TZ='Europe/London' gatsby develop",
    "start:static": "cd docs; ws --spa index.html",
    "serve": "gatsby serve",
    "start:preview": "yarn clean:dist && CI=true gatsby develop -p ${NOMAD_PORT_HTTP} --host 0.0.0.0",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true TZ='Europe/London' gatsby build --log-pages",
    "build:prod": "yarn clean:dist && NODE_ENV=production TZ='Europe/London' gatsby build --prefix-paths && mv ./public ./docs",
    "--- util ---": "",
    "clean": "yarn clean:dist && yarn clean:yarn && lerna clean --yes && yarn clean:npm",
    "clean:dist": "rimraf public && rimraf docs && rimraf ./.cache",
    "clean:npm": "rimraf packages/**/node_modules && rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock && rimraf packages/**/yarn.lock",
    "verify": "yarn lint && yarn test",
    "nuke": "yarn clean && yarn && yarn verify",
    "lerna": "lerna",
    "--- testing ---": "",
    "test": "jest --config=jest.config.js",
    "lint": "eslint . --ext=js,jsx"
  },
  "workspaces": [
    "packages/config/*",
    "packages/helpers/*",
    "packages/*"
  ],
  "dependencies": {
    "@mdx-js/mdx": "1.6.18",
    "@mdx-js/react": "1.6.18",
    "axios": "0.20.0",
    "classnames": "2.2.6",
    "express-ws": "4.0.0",
    "gatsby": "2.24.80",
    "gatsby-link": "2.4.15",
    "gatsby-plugin-buildtime-timezone": "1.0.3",
    "gatsby-plugin-google-analytics": "^2.3.15",
    "gatsby-plugin-react-css-modules": "^2.3.10",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.13",
    "gatsby-plugin-sass": "2.3.17",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.13",
    "gatsby-source-filesystem": "2.3.35",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "postcss-hexrgba": "2.0.1",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.2.3",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-cookie": "4.0.3",
    "react-dom": "16.14.0",
    "react-helmet": "5.2.1",
    "react-query": "2.23.1",
    "react-select": "3.1.0",
    "recharts": "1.8.5",
    "reselect": "4.0.0",
    "rimraf": "3.0.2",
    "url-search-params-polyfill": "8.1.0"
  },
  "devDependencies": {
    "@clearscore/sonar": "2.2.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.2",
    "babel-preset-gatsby": "0.5.14",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.4",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "jest-enzyme": "7.1.2",
    "jest-junit": "11.1.0",
    "lerna": "3.22.1",
    "local-web-server": "4.2.1",
    "netlify-cli": "2.65.6"
  }
}
