{
  "name": "kammy-ssg",
  "version": "0.1.0",
  "description": "UI components + lambdas Kammy using lerna",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "author": "peter.mouland@gmail.com",
  "license": "ISC",
  "homepage": "https://github.com/peter-mouland/kammy-ssg#readme",
  "engines": {
    "node": "18.12.1",
    "npm": "7.0.0",
    "yarn": "1.22.10"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.10"
  },
  "bugs": {
    "url": "https://github.com/peter-mouland/kammy-ssg/issues"
  },
  "scripts": {
    "--- NETLIFY ---": "",
    "dev": "netlify dev",
    "deploy": "yarn build:prod && netlify deploy --prod --open --dir ./public",
    "deploy:hard": "yarn build:prod && netlify deploy --prod --open --dir ./public",
    "--- GATSBY ---": "",
    "start": "TZ='Europe/London' gatsby develop",
    "start:static": "cd public; ws --spa index.html",
    "serve": "gatsby serve",
    "start:preview": "CI=true gatsby develop -p ${NOMAD_PORT_HTTP} --host 0.0.0.0",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "gatsby clean && TZ='Europe/London' gatsby build --log-pages",
    "build:prod": "gatsby clean && NODE_ENV=production TZ='Europe/London' gatsby build --prefix-paths",
    "--- util ---": "",
    "clean": "yarn clean:yarn && lerna clean --yes && yarn clean:npm",
    "clean:dist": "rimraf public && gatsby clean",
    "clean:npm": "rimraf packages/**/node_modules && rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock && rimraf packages/**/yarn.lock",
    "verify": "yarn lint && yarn test",
    "nuke": "yarn clean && yarn && yarn verify",
    "lerna": "lerna",
    "--- testing ---": "",
    "test": "jest --config=jest.config.js",
    "lint": "eslint . --ext=js,jsx"
  },
  "workspaces": [
    "packages/config/*",
    "packages/helpers/*",
    "packages/*"
  ],
  "dependencies": {
    "axios": "1.2.2",
    "chance": "1.1.9",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "fromnow": "3.0.1",
    "gatsby": "3.15.0",
    "gatsby-link": "3.15.0",
    "gatsby-plugin-buildtime-timezone": "1.0.3",
    "gatsby-plugin-gatsby-cloud": "2.11.0",
    "gatsby-plugin-google-analytics": "3.15.0",
    "gatsby-plugin-react-css-modules": "3.15.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-remove-trailing-slashes": "3.15.0",
    "gatsby-plugin-sass": "3.2.0",
    "gatsby-source-filesystem": "3.15.0",
    "node-gyp": "9.3.1",
    "node-sass": "8.0.0",
    "postcss-hexrgba": "2.0.1",
    "postcss-nested": "5.0.5",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-query": "2.26.4",
    "react-select": "4.3.1",
    "recharts": "2.0.9",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "1.5.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-enzyme": "7.1.2",
    "jest-junit": "15.0.0",
    "lerna": "6.3.0",
    "local-web-server": "5.2.1",
    "netlify-cli": "12.5.0"
  }
}
