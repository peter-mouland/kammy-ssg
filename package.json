{
  "name": "kammy-ssg",
  "version": "0.1.0",
  "description": "UI components + lambdas Kammy using lerna",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "author": "peter.mouland@gmail.com",
  "license": "ISC",
  "homepage": "https://github.com/peter-mouland/kammy-ssg#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.22.0"
  },
  "bugs": {
    "url": "https://github.com/peter-mouland/kammy-ssg/issues"
  },
  "scripts": {
    "--- NETLIFY ---": "",
    "dev": "netlify dev",
    "deploy": "yarn build:prod && netlify deploy --prod --open --dir ./docs",
    "deploy:hard": "yarn clean:deploy && yarn build:prod && netlify deploy --prod --open --dir ./docs",
    "--- GATSBY ---": "",
    "start": "yarn clean:deploy && TZ='Europe/London' gatsby develop",
    "start:static": "cd docs; ws --spa index.html",
    "serve": "gatsby serve",
    "start:preview": "yarn clean:deploy && CI=true gatsby develop -p ${NOMAD_PORT_HTTP} --host 0.0.0.0",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "TZ='Europe/London' gatsby build --log-pages",
    "build:prod": "NODE_ENV=production TZ='Europe/London' gatsby build --prefix-paths",
    "--- util ---": "",
    "clean": "yarn clean:deploy && yarn clean:yarn && lerna clean --yes && yarn clean:npm",
    "clean:dist": "rimraf public && rimraf docs && rimraf ./.cache",
    "clean:npm": "rimraf packages/**/node_modules && rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock && rimraf packages/**/yarn.lock",
    "verify": "yarn lint && yarn test",
    "nuke": "yarn clean && yarn && yarn verify",
    "lerna": "lerna",
    "--- testing ---": "",
    "test": "jest --config=jest.config.js",
    "lint": "eslint . --ext=js,jsx"
  },
  "workspaces": [
    "packages/config/*",
    "packages/helpers/*",
    "packages/*"
  ],
  "dependencies": {
    "axios": "0.21.1",
    "chance": "1.1.7",
    "classnames": "2.2.6",
    "date-fns": "2.19.0",
    "dotenv": "8.2.0",
    "fromnow": "3.0.1",
    "gatsby": "3.0.4",
    "gatsby-link": "3.0.1",
    "gatsby-plugin-buildtime-timezone": "1.0.3",
    "gatsby-plugin-google-analytics": "3.0.0",
    "gatsby-plugin-react-css-modules": "3.0.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-remove-trailing-slashes": "3.0.0",
    "gatsby-plugin-sass": "4.0.2",
    "gatsby-source-filesystem": "3.0.0",
    "node-sass": "5.0.0",
    "postcss-hexrgba": "2.0.1",
    "postcss-nested": "5.0.5",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-cookie": "4.0.3",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-query": "2.26.4",
    "react-select": "4.2.1",
    "recharts": "2.0.8",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@clearscore/sonar": "3.3.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "1.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-enzyme": "7.1.2",
    "jest-junit": "12.0.0",
    "lerna": "4.0.0",
    "local-web-server": "4.2.1",
    "netlify-cli": "3.10.9"
  }
}
